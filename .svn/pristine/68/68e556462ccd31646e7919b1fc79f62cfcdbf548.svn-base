package com.hotelsystem.utils;

import org.springframework.stereotype.Component;

@Component
public class CompareTime {
	public boolean compareYear(String str1,String str2){
		if(str1.substring(0, 4).equals(str2.substring(0, 4))){
			return true;
		}else{
			return false;
		}
	}
	public boolean compareMonth(String str1,String str2){
		if(str1.substring(5, 7).equals(str2.substring(5, 7))){
			return true;
		}else{
			return false;
		}
	}
	public boolean compareDay(String str1,String str2){
		if(str1.substring(8, 10).equals(str2.substring(8, 10))){
			return true;
		}else{
			return false;
		}
	}
}
