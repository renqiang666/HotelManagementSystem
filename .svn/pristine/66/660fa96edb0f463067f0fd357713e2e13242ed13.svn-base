 */
public class LevelDiscountBean{

	private int classId;
	private String className;
	private double classDiscount;
	private double classConsume;
	public int getClassId() {
		return classId;
	}

	public void setClassId(int classId) {
		this.classId = classId;
	}

	public String getClassName() {
		return className;
	}

	public void setClassName(String className) {
		this.className = className;
	}

	public double getClassDiscount() {
		return classDiscount;
	}

	public void setClassDiscount(double classDiscount) {
		this.classDiscount = classDiscount;
	}

	public double getClassConsume() {
		return classConsume;
	}

	public void setClassConsume(double classConsume) {
		this.classConsume = classConsume;
	}

	@Override
	public String toString() {
		return "LevelDiscountBean [classId=" + classId + ", className=" + className + ", classDiscount=" + classDiscount
				+ ", classConsume=" + classConsume + "]";
	}


	
}
